
public class ex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ex1 ex = new ex1();
		int data[] = {0,1,1,0,0};
		int answer =ex.solution(data);
		System.out.println(answer);
	}

	  public int solution(int[] fence) {
	      int answer = 0;
	      int count[] = {0,0};
	      // 첫번째 울타리가 흰색
	      if(fence[0] == 0) {
	    	  for(int i = 0; i < fence.length; i++) {
	    		  if(fence[0] != 0) {
	    			  count[0]++;
	    			  fence[0] = 1;
	    		  }
	    		  
	    	  }
	    	  
	      }
	      // 첫번째 울타리가 검은색
	      else {
	    	 
	    	  
	    	  
	    	  
	      }
	     

	      
	      return answer;
	  }
	  
}
